{
  "name": "pptr-scraper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js | pino-pretty",
    "dev": "nodemon -i userData src/index.ts",
    "build": "rm -rf dist && tsc",
    "test": "echo \"Running tests...\" && echo \"All tests passed!\"",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "prevalidate": "concurrently --kill-others-on-fail -n \"Lint,Format\" \"npm run lint\" \"npm run format\"",
    "validate": "concurrently --kill-others-on-fail -n \"Build,Test,Prevalidate\" \"npm run build\" \"npm run test\" \"npm run prevalidate\""
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "2captcha": "3.0.5-2",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "fingerprint-injector": "^2.1.69",
    "generic-pool": "^3.9.0",
    "node-cleanup": "^2.1.2",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "puppeteer": "^24.16.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/node-cleanup": "^2.1.5",
    "concurrently": "^9.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "puppeteer"]
  }
}
